
#include <stdio.h>

int main() {
    int num=19;
    int originalNum=num;
    int sum=0;
    while (num!=1 && num!=4){
       int sum =0;
       while(num>0){
       int digit =num %10; 
    
    sum+=digit*digit; 
    num/=10;}
    num=sum ;
    }
    if(num==1){
        printf("%d is a Happy number.\n",originalNum);
        
            }else{
            
                 printf("%d os not a Happy number./n",originalNum);
            
        
        
           
        

    }
    

    return 0;
}
