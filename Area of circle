#include <stdio.h>
float circleArea (float r);
int main()
{
 float radius ,area;
 printf(" enter the radius of the circle ");
 scanf("%f",& radius);
 area=circleArea(radius);
 printf("area of circle= %2f\n",area);
    return 0;
}
float circleArea (float r)
{
    float area=3.14*r*r;
    return area;
}
